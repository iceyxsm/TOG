// Control flow in TOG
fn main() {
    let x = 10
    
    // If-else
    if x > 5 {
        print("x is greater than 5")
    } else {
        print("x is not greater than 5")
    }
    
    // While loop
    let i = 0
    while i < 5 {
        print(i)
        i = i + 1
    }
    
    // Pattern matching
    match x {
        1 => print("One"),
        10 => print("Ten"),
        _ => print("Other")
    }
}

